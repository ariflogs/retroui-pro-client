#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arifhossain/Projects/ptm/RetroUI/pro/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.10_jiti@2.6.1_jsdom@26.1.0_lightningcss@1.30.1_msw@2.11.5_@typ_7x3kkzk7p2wi2xmetcrttukfji/node_modules/vitest/node_modules:/Users/arifhossain/Projects/ptm/RetroUI/pro/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.10_jiti@2.6.1_jsdom@26.1.0_lightningcss@1.30.1_msw@2.11.5_@typ_7x3kkzk7p2wi2xmetcrttukfji/node_modules:/Users/arifhossain/Projects/ptm/RetroUI/pro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arifhossain/Projects/ptm/RetroUI/pro/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.10_jiti@2.6.1_jsdom@26.1.0_lightningcss@1.30.1_msw@2.11.5_@typ_7x3kkzk7p2wi2xmetcrttukfji/node_modules/vitest/node_modules:/Users/arifhossain/Projects/ptm/RetroUI/pro/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.10_jiti@2.6.1_jsdom@26.1.0_lightningcss@1.30.1_msw@2.11.5_@typ_7x3kkzk7p2wi2xmetcrttukfji/node_modules:/Users/arifhossain/Projects/ptm/RetroUI/pro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
